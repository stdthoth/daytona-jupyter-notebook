{
  "name": "Jupyter Dev Container",
  "dockerComposeFile": "docker-compose.yml",
  "service": "jupyter",
  "workspaceFolder": "/home/jovyan/work",
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "postCreateCommand": "echo 'jovyan ALL=(ALL) NOPASSWD: ALL' | sudo tee /etc/sudoers.d/jovyan"
}

  
